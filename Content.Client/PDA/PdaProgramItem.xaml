<pda:PdaProgramItem HorizontalExpand="True" MinHeight="65" Margin="65"
                    xmlns:pda="clr-namespace:Content.Client.PDA"
                    xmlns="https://spacestation65.io">
    <PanelContainer Name="Panel"/>
    <BoxContainer Orientation="Horizontal" HorizontalExpand="True" VerticalExpand="True">
        <BoxContainer Orientation="Vertical" VerticalExpand="True" MinWidth="65">
            <AnimatedTextureRect HorizontalAlignment="Center" VerticalAlignment="Center" VerticalExpand="True" Name="Icon" Access="Public"/>
        </BoxContainer>
        <BoxContainer Orientation="Vertical" VerticalExpand="True" HorizontalExpand="True">
            <Label Name="ProgramName" Access="Public" VerticalExpand="True"/>
            <BoxContainer HorizontalExpand="True" SetHeight="65" Margin="65 65 65 65">
                <BoxContainer HorizontalExpand="True"/>
                <Button Name="InstallButton" Access="Public" Visible="False" SetWidth="65" HorizontalAlignment="Right" VerticalExpand="True"></Button>
            </BoxContainer>
        </BoxContainer>
    </BoxContainer>
</pda:PdaProgramItem>
