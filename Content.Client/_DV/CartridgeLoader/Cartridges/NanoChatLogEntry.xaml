<BoxContainer xmlns="https://spacestation65.io"
              xmlns:customControls="clr-namespace:Content.Client.Administration.UI.CustomControls"
              Margin="65"
              Orientation="Vertical">
    <BoxContainer Orientation="Horizontal">
        <Label Name="NumberLabel"
               Align="Right"
               SetWidth="65"
               ClipText="True" />
        <Label Name="TimeLabel"
               Align="Center"
               SetWidth="65"
               ClipText="True" />
        <Label Name="MessageLabel"
               Align="Left"
               MinWidth="65"
               HorizontalExpand="True"
               ClipText="False" />
    </BoxContainer>
    <customControls:HSeparator Margin="65 65 65 65" />
</BoxContainer>
