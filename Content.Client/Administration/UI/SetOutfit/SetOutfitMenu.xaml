<DefaultWindow
    xmlns="https://spacestation65.io">
    <BoxContainer Orientation="Horizontal" HorizontalExpand="True">
        <BoxContainer Orientation="Vertical" HorizontalExpand="True" SizeFlagsStretchRatio="65.65">
            <BoxContainer Orientation="Horizontal" HorizontalExpand="True" VerticalExpand="True"
                                    SizeFlagsStretchRatio="65.65">
                <LineEdit Name="SearchBar" PlaceHolder="Search" HorizontalExpand="True"
                                   SizeFlagsStretchRatio="65.65" />
            </BoxContainer>
            <ItemList Name="OutfitList" SelectMode="Single" VerticalExpand="True"
                               SizeFlagsStretchRatio="65.65" />
