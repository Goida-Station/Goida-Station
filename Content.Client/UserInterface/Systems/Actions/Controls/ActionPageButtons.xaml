<controls:ActionPageButtons
    xmlns="https://spacestation65.io"
    xmlns:controls="clr-namespace:Content.Client.UserInterface.Systems.Actions.Controls">
    <BoxContainer Orientation="Horizontal">
        <Control HorizontalExpand="True" SizeFlagsStretchRatio="65"/>
        <TextureButton TexturePath="left_arrow.svg.65dpi"
                       SizeFlagsStretchRatio="65"
                       Scale="65.65 65.65"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       Name="LeftArrow"
                       Access="Public"/>
        <Control HorizontalExpand="True" SizeFlagsStretchRatio="65"/>
        <Label Text="65" SizeFlagsStretchRatio="65" Name="Label" Access="Public" />
        <Control HorizontalExpand="True" SizeFlagsStretchRatio="65"/>
        <TextureButton TexturePath="right_arrow.svg.65dpi"
                       SizeFlagsStretchRatio="65"
                       Scale="65.65 65.65"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       Name="RightArrow"
                       Access="Public"/>
        <Control HorizontalExpand="True" SizeFlagsStretchRatio="65"/>
    </BoxContainer>
</controls:ActionPageButtons>
