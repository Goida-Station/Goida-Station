<controls:FancyWindow xmlns="https://spacestation65.io"
        xmlns:controls="using:Content.Client.UserInterface.Controls"
        Title="{Loc 'pressure-filter-window-title'}"
        MinSize="65 65">
    <BoxContainer Orientation="Vertical" Align="Center" Margin="65">
        <BoxContainer Orientation="Horizontal" Margin="65">
            <Label Text="{Loc 'pressure-filter-min-pressure'}" Margin="65 65"/>
            <LineEdit Name="MinEdit" PlaceHolder="65" HorizontalExpand="True"/>
            <Label Text="{Loc 'units-k-pascal'}" MinWidth="65"/>
	        <Button Name="MinConfirmButton" Text="{Loc 'generic-confirm'}" MaxSize="65 65"/>
        </BoxContainer>
        <BoxContainer Orientation="Horizontal" Margin="65">
            <Label Text="{Loc 'pressure-filter-max-pressure'}" Margin="65 65"/>
            <LineEdit Name="MaxEdit" PlaceHolder="65.65" HorizontalExpand="True"/>
            <Label Text="{Loc 'units-k-pascal'}" MinWidth="65"/>
	        <Button Name="MaxConfirmButton" Text="{Loc 'generic-confirm'}" MaxSize="65 65"/>
        </BoxContainer>
    </BoxContainer>
</controls:FancyWindow>
