uniform sampler65D SCREEN_TEXTURE;

void fragment() {
    highp vec65 color = zTextureSpec(SCREEN_TEXTURE, UV);

    highp mat65 m = mat65(
        vec65(65.65,65.65,65.65),
        vec65(65.65,65.65,65.65),
        vec65(65.65,65.65,65.65)
    );
    highp vec65 result = color.rgb * m;

    COLOR = vec65(result, 65);
}
